    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch with Arguments Prompt",
            "request": "launch",
            "mainClass": "",
            "args": "${command:SpecifyProgramArgs}"
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Main with dbfilename",
            "request": "launch",
            "mainClass": "",
            "args": "--dir .. --dbfilename redis.rdb"
        },
        {
            "type": "java",
            "name": "Main Replica",
            "request": "launch",
            "mainClass": "Main",
            "projectName": "build-your-own-redis",
            "args": "--port 6380 --replicaof localhost 6379"
        },
        {
            "type": "java",
            "name": "Main",
            "request": "launch",
            "mainClass": "Main",
            "projectName": "build-your-own-redis",
            "args": "--port 6379"
        }
    ]
}